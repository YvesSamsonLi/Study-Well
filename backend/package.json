{
  "scripts": {
    "dev": "tsx watch src/app/server.ts",
    "dev:once": "tsx src/app/server.ts",
    "dev:reminders": "tsx src/jobs/workers.reminders.ts",
    "dev:sessionCleanup": "tsx src/jobs/workers.sessionCleanup.ts",
    "prisma:validate": "prisma validate --schema=prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema=prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=prisma/schema.prisma --force",
    "db:seed": "tsx src/scripts/seeds.ts && tsx src/scripts/seeds_calendar.ts",
    "db:reset:seed": "npm run db:reset && npm run db:seed"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.0",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/swagger": "^9.0.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.62.0",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.1.0",
    "googleapis": "^164.1.0",
    "install": "^0.13.0",
    "ioredis": "^5.8.2",
    "nodemailer": "^7.0.10",
    "pdf-parse": "^2.4.5",
    "pino": "^9.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "axios": "^1.13.1",
    "form-data": "^4.0.4",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.17.1",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "tsx src/scripts/seeds.ts && tsx src/scripts/seeds_calendar.ts"
  }
}
